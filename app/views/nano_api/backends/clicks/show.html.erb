<% if 'get' == @click_form[:method] and @click_form[:params].empty? %>
  <script type="text/javascript">
    window.location.href = '<%=raw @click_form[:url] %>';
  </script>
<% else %>
  <%= form_tag @click_form[:url], method: @click_form[:method], id: 'nano_api_click_form' do %>
    <% @click_form[:params].each do |key, value| %>
      <%= hidden_field_tag key, h(value) %>
    <% end %>
  <% end %>

  <script type="text/javascript">
    document.getElementById('nano_api_click_form').submit();
  </script>
<% end %>
